<%- jsLsload({path:('js/jquery.pjax.js'),key:'pjax_js'}) %>

<script type="text/ls-javascript" id="Pjax-script">
    $(document).pjax('a', '#main', {fragment: '#main'})
              .on('pjax:start', function () { NProgress.start() })
              .on('pjax:end', function () { NProgress.done() });
</script>