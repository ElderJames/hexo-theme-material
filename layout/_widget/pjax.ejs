<script src="//cdn.bootcss.com/jquery.pjax/1.9.6/jquery.pjax.min.js"></script>
<script>
    $(document).pjax('a', '.pjax-fragment', {
        fragment: '.pjax-fragment',
        timeout: 5000,
    }) .on('pjax:start', function () { NProgress.start() })
       .on('pjax:end', function () { NProgress.done() });;
</script>